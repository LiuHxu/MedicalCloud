<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>CloudServiceEngineering</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>org.cloudserviceengineering.app.CloudServiceEngineering</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserManageService_Tool</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>org.cloudserviceengineering.app.UserManageService_Tool</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>CloudServiceEngineering</servlet-name>
    <url-pattern>/servlet/CloudServiceEngineering</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserManageService_Tool</servlet-name>
    <url-pattern>/servlet/UserManageService_Tool</url-pattern>
  </servlet-mapping>

</web-app>